{
  "schemaVersion": 1,
  "id": "hexdebug",
  "version": "${modVersion}",
  "name": "HexDebug",
  "description": "The HexDebug addon for Hex Casting",
  "authors": [
    "object-Object"
  ],
  "contact": {
    "homepage": "https://hexdebug.hexxy.media",
    "sources": "https://github.com/object-Object/HexDebug"
  },
  "license": "MIT",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [{
      "adapter": "kotlin",
      "value": "gay.object.hexdebug.fabric.FabricHexDebug"
    }],
    "client": [{
      "adapter": "kotlin",
      "value": "gay.object.hexdebug.fabric.FabricHexDebugClient"
    }],
    "server": [{
      "adapter": "kotlin",
      "value": "gay.object.hexdebug.fabric.FabricHexDebugServer"
    }],
    "fabric-datagen": [{
      "adapter": "kotlin",
      "value": "gay.object.hexdebug.fabric.datagen.FabricHexDebugDatagen"
    }],
    "emi": [{
      "adapter": "kotlin",
      "value": "gay.object.hexdebug.fabric.interop.FabricHexDebugEmiPlugin"
    }],
    "modmenu": [{
      "adapter": "kotlin",
      "value": "gay.object.hexdebug.fabric.interop.FabricHexDebugModMenu"
    }]
  },
  "mixins": [
    "hexdebug-common.mixins.json"
  ],
  "depends": {
    "minecraft": "${versions.minecraft}",
    "fabricloader": ">=${versions.fabric_loader}",
    "fabric-api": ">=${versions.fabric_api}",
    "fabric-language-kotlin": ">=${versions.kotlin_fabric}",
    "architectury": ">=${versions.architectury}",
    "hexcasting": ">=${versions.hexcasting_dependency}",
    "cloth-config": ">=${versions.clothConfig}",
    "ioticblocks": ">=${versions.ioticblocks}"
  },
  "suggests": {
    "emi": ">=${versions.emi}",
    "modmenu": ">=${versions.modMenu}",
    "hexical": ">=${versions.hexical}"
  },
  "conflicts": {
    "hexical": "<${versions.hexical}"
  }
}
